{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "users-route": {
        "ClusterId": "users-cluster",
        "Match": {
          "Path": "/users/{**catch-all}"
        }
      },
      "events-route": {
        "ClusterId": "events-cluster",
        "Match": {
          "Path": "/events/{**catch-all}"
        }
      },
      "notifications-route": {
        "ClusterId": "notifications-cluster",
        "Match": {
          "Path": "/notifications/{**catch-all}"
        }
      },
      "payments-route": {
        "ClusterId": "payments-cluster",
        "Match": {
          "Path": "/payments/{**catch-all}"
        }
      }
    }
  },
  "Clusters": {
    "users-cluster": {
      "Destinations": {
        "destination1": {
          "Address": "http://localhost:5038"
        }
      }
    },
    "events-cluster": {
      "Destinations": {
        "destination1": {
          "Address": "http://localhost:5040"
        }
      }
    },
    "notifications-cluster": {
      "Destinations": {
        "destination1": {
          "Address": "http://localhost:5042"
        }
      }
    }
  },
  "payments-cluster": {
    "Destinations": {
      "destination1": {
        "Address": "http://localhost:5044"
      }
    }
  }
}